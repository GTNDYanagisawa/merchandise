<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="entitlement" />
		<property name="aliasedClass" value="de.hybris.platform.entitlementfacades.data.EntitlementData" />
	</bean>

	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="product" />
		<property name="aliasedClass" value="de.hybris.platform.commercefacades.product.data.ProductData" />
	</bean>

	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="Image" />
		<property name="aliasedClass" value="de.hybris.platform.commercefacades.product.data.ImageData" />
	</bean>

</beans>
